<aura:component controller="GoogleCloudStorageController">
	<aura:attribute name="storageItem" type="Cloud_Storage_Item__c" />
	<!-- <aura:attribute name="downloadLink" type="String" 
    default="{'sobjectType':'String'}"/> -->

    <tr class="slds-hint-parent slds-has-flexi-truncate">
        <th scope="row" data-label="Name">
          <div class="slds-truncate" title="Name">
            <a href="javascript:void(0);">{!v.storageItem.Name}</a>
        </div>
        </th>
        <td data-label="Status">
            <div class="slds-truncate" title="Status">{!v.storageItem.Status__c}</div>
        </td>
        <td data-label="File Type">
            <div class="slds-truncate" title="File Type">{!v.storageItem.File_Type__c}</div>
        </td>
        <td data-label="Size">
            <div class="slds-truncate" title="Size">{!v.storageItem.Size__c}</div>
        </td>
        <td data-label="Uploaded On">
            <div class="slds-truncate" title="Uploaded On">{!v.storageItem.Uploaded_On__c}</div>
        </td>
    </tr>

</aura:component>